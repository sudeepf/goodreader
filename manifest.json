{
  "manifest_version": 2,

  "name": "goodReader",
  "description": "This extention provides a simple yet rich interface with your goodreads account!",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icons/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "https://*.goodreads.com/*"
  ]
}
